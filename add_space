def add_spaces(text):
  spaced = ""
  for char in text:
    spaced += char + " "
  return spaced[:-1]

string = "CCCCCHHHHHHHHHHHHHHTTCCCTTCCCCCCHHHHHHHTHHHHHHHHHHHHHHHH HHHHHHHHHHCCCCHHHHHHHHHHHHHHHHHHHHHHHHHHTHHHHHHHHHHHHHHH HHHCCCHHHHHHHHHHHHHHHHHH HHHHHHHH HHHHHHHH HHHHHHHH HHHHHCCC CCCCCCCC CCCCCTTT CHHHHHHH HHHHHHHH HHHHHHHH HHHHHHHH HHHHHHHH HHHHHHHH HTCCCCCC CCCCCHHH HHHHHHHH HHHHHHHH HHHHHHHH HHHHHHHH HHHHHHHH HHHHHHHH HHCCTHHH HHHHHHHH HHHHHHHH HHHHHHHH HHHHHHHH HHHHTCCH HHCCCHHH HHHHHHHH HHHTCCCC CEETTTEE CCHHHHHH HHHHHHHH HHHHHHHH HTC"
text = string.replace(" ","")

spaced_text = add_spaces(text)
print(spaced_text)

